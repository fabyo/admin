{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
		"predis/predis": "dev-master",
        "pusher/pusher-php-server": "^2.4",
		"php-console/php-console": "~3.0",
        "guzzlehttp/guzzle": "^6.2",
        "league/fractal": "^0.13.0",
        "league/flysystem-ziparchive": "^1.0",
        "graham-campbell/exceptions": "^9.0",
        "filp/whoops": "^2.1",
        "whoops/soap": "^1.0",
        "vinkla/backup": "^1.2",
        "vinkla/pusher": "^2.4",
        "owen-it/laravel-auditing": "^2.4",
		"maatwebsite/excel": "~2.1.0",
        "intervention/image": "^2.3",
        "barryvdh/laravel-snappy": "^0.3.0",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "^2.5",
		"kriansa/openboleto": "dev-master",
        "eduardokum/laravel-boleto": "^0.3.1",
		"caouecs/laravel-lang": "~3.0",
        "nfephp-org/nfephp": "^4.0",
        "beberlei/assert": "^2.6",
		"laravelcollective/html": "5.2.*",
        "prettus/l5-repository": "^2.6",
        "zizaco/entrust": "5.2.x-dev"		
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "FGO\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan ide-helper:generate",
			"php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
